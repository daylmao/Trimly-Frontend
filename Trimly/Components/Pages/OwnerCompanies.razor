@page "/owner/companies"

<body class="min-h-screen bg-background text-text">
<div class="flex h-screen overflow-hidden">
  <!-- Sidebar -->
  <aside class="hidden md:flex md:flex-col w-64 bg-surface border-r border-gray-700">
    <!-- Logo -->
    <div class="p-4 border-b border-gray-700">
      <NavLink href="/" class="text-2xl font-bold bg-gradient-to-r from-primary to-secondary bg-clip-text text-transparent">
        Trimly
      </NavLink>
    </div>

    <!-- Navigation -->
    <nav class="flex-1 overflow-y-auto py-4">
      <ul class="space-y-1 px-2">
        <li>
          <NavLink href="/owner-dashboard" class="flex items-center px-4 py-2 text-sm rounded-md hover:bg-gray-700">
            <i class="fas fa-tachometer-alt w-5 h-5 mr-3"></i>
            Dashboard
          </NavLink>
        </li>
        <li>
          <NavLink href="/owner/companies" class="flex items-center px-4 py-2 text-sm rounded-md bg-primary/10 text-primary">
            <i class="fas fa-store w-5 h-5 mr-3"></i>
            Mis Barberías
          </NavLink>
        </li>
        <li>
          <NavLink href="/owner-barberos" class="flex items-center px-4 py-2 text-sm rounded-md hover:bg-gray-700">
            <i class="fas fa-cut w-5 h-5 mr-3"></i>
            Barberos
          </NavLink>
        </li>
        <li>
          <NavLink href="/owner/services" class="flex items-center px-4 py-2 text-sm rounded-md hover:bg-gray-700">
            <i class="fas fa-list-alt w-5 h-5 mr-3"></i>
            Servicios
          </NavLink>
        </li>
        <li>
          <NavLink href="/owner-appointments.html" class="flex items-center px-4 py-2 text-sm rounded-md hover:bg-gray-700">
            <i class="fas fa-calendar-alt w-5 h-5 mr-3"></i>
            Citas
          </NavLink>
        </li>

        <li>
          <NavLink href="/owner/reviews" class="flex items-center px-4 py-2 text-sm rounded-md hover:bg-gray-700">
            <i class="fas fa-star w-5 h-5 mr-3"></i>
            Reseñas
          </NavLink>
        </li>

      </ul>

      <div class="mt-8 px-4">
        <h3 class="px-2 text-xs font-semibold text-gray-400 uppercase tracking-wider">
          Configuración
        </h3>
        <ul class="mt-2 space-y-1">
          <li>
            <NavLink href="/owner/profile" class="flex items-center px-4 py-2 text-sm rounded-md hover:bg-gray-700">
              <i class="fas fa-user-circle w-5 h-5 mr-3"></i>
              Mi Perfil
            </NavLink>
          </li>
          <li>
            <NavLink href="/owner/settings" class="flex items-center px-4 py-2 text-sm rounded-md hover:bg-gray-700">
              <i class="fas fa-cog w-5 h-5 mr-3"></i>
              Ajustes
            </NavLink>
          </li>
        </ul>
      </div>
    </nav>

    <!-- User -->
    <div class="p-4 border-t border-gray-700">
      <div class="flex items-center">
        <img src="https://via.placeholder.com/40" alt="Avatar" class="w-8 h-8 rounded-full mr-3">
        <div>
          <p class="text-sm font-medium">Juan Pérez</p>
          <p class="text-xs text-gray-400">Propietario</p>
        </div>
        <button id="logout-sidebar-button" class="ml-auto text-gray-400 hover:text-white">
          <i class="fas fa-sign-out-alt"></i>
        </button>
      </div>
    </div>
  </aside>

  <!-- Mobile sidebar toggle -->
  <div class="md:hidden fixed bottom-4 right-4 z-50">
    <button id="mobile-menu-button" class="bg-primary text-white p-3 rounded-full shadow-lg">
      <i class="fas fa-bars"></i>
    </button>
  </div>

  <!-- Mobile sidebar (hidden by default) -->
  <div id="mobile-sidebar" class="fixed inset-0 z-40 hidden">
    <div class="absolute inset-0 bg-black bg-opacity-50" id="mobile-sidebar-backdrop"></div>
    <div class="absolute inset-y-0 left-0 w-64 bg-surface transform transition-transform duration-300 -translate-x-full" id="mobile-sidebar-content">
      <!-- Mobile sidebar content (same as desktop) -->
      <!-- Logo -->
      <div class="p-4 border-b border-gray-700 flex justify-between items-center">
        <a href="/" class="text-2xl font-bold bg-gradient-to-r from-primary to-secondary bg-clip-text text-transparent">
          Trimly
        </a>
        <button id="close-mobile-menu" class="text-gray-400 hover:text-white">
          <i class="fas fa-times"></i>
        </button>
      </div>

      <!-- Navigation -->
      <nav class="flex-1 overflow-y-auto py-4">
        <ul class="space-y-1 px-2">
          <li>
            <a href="/owner-dashboard.html" class="flex items-center px-4 py-2 text-sm rounded-md hover:bg-gray-700">
              <i class="fas fa-tachometer-alt w-5 h-5 mr-3"></i>
              Dashboard
            </a>
          </li>
          <li>
            <a href="/owner-companies.html" class="flex items-center px-4 py-2 text-sm rounded-md bg-primary/10 text-primary">
              <i class="fas fa-store w-5 h-5 mr-3"></i>
              Mis Barberías
            </a>
          </li>
          <li>
            <a href="/owner-barberos.html" class="flex items-center px-4 py-2 text-sm rounded-md hover:bg-gray-700">
              <i class="fas fa-cut w-5 h-5 mr-3"></i>
              Barberos
            </a>
          </li>
          <li>
            <a href="/owner-servicios.html" class="flex items-center px-4 py-2 text-sm rounded-md hover:bg-gray-700">
              <i class="fas fa-list-alt w-5 h-5 mr-3"></i>
              Servicios
            </a>
          </li>
          <li>
            <a href="/owner-appointments.html" class="flex items-center px-4 py-2 text-sm rounded-md hover:bg-gray-700">
              <i class="fas fa-calendar-alt w-5 h-5 mr-3"></i>
              Citas
            </a>
          </li>

          <li>
            <a href="/owner-reviews.html" class="flex items-center px-4 py-2 text-sm rounded-md hover:bg-gray-700">
              <i class="fas fa-star w-5 h-5 mr-3"></i>
              Reseñas
            </a>
          </li>

        </ul>

        <div class="mt-8 px-4">
          <h3 class="px-2 text-xs font-semibold text-gray-400 uppercase tracking-wider">
            Configuración
          </h3>
          <ul class="mt-2 space-y-1">
            <li>
              <a href="/owner-perfil.html" class="flex items-center px-4 py-2 text-sm rounded-md hover:bg-gray-700">
                <i class="fas fa-user-circle w-5 h-5 mr-3"></i>
                Mi Perfil
              </a>
            </li>
            <li>
              <a href="/owner-ajustes.html" class="flex items-center px-4 py-2 text-sm rounded-md hover:bg-gray-700">
                <i class="fas fa-cog w-5 h-5 mr-3"></i>
                Ajustes
              </a>
            </li>
          </ul>
        </div>
      </nav>

      <!-- User -->
      <div class="p-4 border-t border-gray-700">
        <div class="flex items-center">
          <img src="https://via.placeholder.com/40" alt="Avatar" class="w-8 h-8 rounded-full mr-3">
          <div>
            <p class="text-sm font-medium">Juan Pérez</p>
            <p class="text-xs text-gray-400">Propietario</p>
          </div>
          <button id="logout-mobile-button" class="ml-auto text-gray-400 hover:text-white">
            <i class="fas fa-sign-out-alt"></i>
          </button>
        </div>
      </div>
    </div>
  </div>

  <!-- Main Content -->
  <div class="flex-1 flex flex-col overflow-hidden">
    <!-- Top Header -->
    <header class="bg-surface/80 backdrop-blur-md shadow-md z-10">
      <div class="px-4 py-4 flex justify-between items-center">
        <div>
          <h1 class="text-xl font-semibold">Mis Barberías</h1>
          <p class="text-sm text-gray-400">Gestiona tus establecimientos</p>
        </div>

        <div class="flex items-center space-x-4">

        </div>
      </div>
    </header>

    <!-- Main Content Area -->
    <main class="flex-1 overflow-y-auto bg-background p-4">
      <!-- Acciones y Filtros -->
      <div class="bg-surface rounded-lg shadow-md p-4 mb-6">
        <div class="flex flex-col md:flex-row md:items-center md:justify-between gap-4">
          <div class="flex flex-col md:flex-row gap-4 md:items-center">
            <!-- Búsqueda -->
            <div class="w-full md:w-64">
              <div class="relative">
                <input
                  type="text"
                  placeholder="Buscar barbería..."
                  class="w-full bg-gray-700 border border-gray-600 rounded-md pl-10 pr-3 py-2 text-sm focus:outline-none focus:ring-2 focus:ring-primary"
                />
                <div class="absolute inset-y-0 left-0 flex items-center pl-3 pointer-events-none">
                  <i class="fas fa-search text-gray-400"></i>
                </div>
              </div>
            </div>

            <!-- Filtro por Estado -->
            <div class="w-full md:w-48">
              <select class="w-full bg-gray-700 border border-gray-600 rounded-md px-3 py-2 text-sm focus:outline-none focus:ring-2 focus:ring-primary">
                <option value="all">Todos los estados</option>
                <option value="active">Activas</option>
                <option value="inactive">Inactivas</option>
                <option value="pending">Pendientes</option>
              </select>
            </div>
          </div>

          <!-- Botón Añadir Barbería -->
          <div class="mt-4 md:mt-0">
            <button id="add-barbershop-button" class="bg-primary hover:bg-primary/90 text-white px-4 py-2 rounded-md flex items-center">
              <i class="fas fa-plus mr-2"></i>
              Añadir Barbería
            </button>
          </div>
        </div>
      </div>

      <!-- Lista de Barberías -->
      <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-6">
        <!-- Barbería 1 -->
        <div class="bg-surface rounded-lg shadow-md overflow-hidden transition-transform duration-300 flex flex-col h-full">
          <!-- Sección de imagen -->
          <div class="h-48 relative flex-shrink-0">
            <img
              src="https://via.placeholder.com/800x400"
              alt="Barbería Moderna"
              class="w-full h-full object-cover"
            />
            <!-- Badge de estado -->
            <div class="absolute top-2 right-2">
              <span class="px-2 py-1 bg-green-500/90 text-white rounded-full text-xs font-medium shadow-sm">Activa</span>
            </div>
          </div>

          <!-- Contenido de la tarjeta -->
          <div class="p-4 flex flex-col flex-grow">
            <!-- Encabezado con nombre y rating -->
            <div class="flex justify-between items-start mb-4">
              <h3 class="text-lg font-semibold text-gray-100">Barbería Moderna</h3>
              <div class="flex items-center bg-gray-700/50 px-2 py-1 rounded-md">
                <i class="fas fa-star text-yellow-400 mr-1 text-sm"></i>
                <span class="text-sm font-medium">4.8</span>
              </div>
            </div>

            <!-- Dirección -->
            <p class="text-sm text-gray-400 mb-4 flex items-center">
              <i class="fas fa-map-marker-alt mr-2 text-gray-500"></i>
              Calle Gran Vía 28, Madrid
            </p>

            <!-- Botones de acción -->
            <div class="mt-auto flex space-x-2">
              <!-- Botón principal -->
              <a href="/owner-company-details.html" class="flex-1 bg-primary/10 hover:bg-primary/20 text-primary text-center py-2 rounded-md text-sm font-medium transition-colors duration-200">
                Ver detalles
              </a>

              <!-- Botones secundarios -->
              <button class="p-2 bg-gray-700 hover:bg-gray-600 rounded-md transition-colors duration-200" aria-label="Editar">
                <i class="fas fa-edit text-gray-300 text-sm"></i>
              </button>
              <button class="p-2 bg-gray-700 hover:bg-gray-600 rounded-md transition-colors duration-200" aria-label="Configuración">
                <i class="fas fa-cog text-gray-300 text-sm"></i>
              </button>
            </div>
          </div>
        </div>

        <!-- Nueva Barbería (Añadir) -->
        <div class="bg-surface/50 border-2 border-dashed border-gray-600 rounded-lg flex flex-col items-center justify-center p-6 h-full">
          <div class="w-16 h-16 rounded-full bg-primary/10 flex items-center justify-center mb-4">
            <i class="fas fa-plus text-primary text-xl"></i>
          </div>
          <h3 class="text-lg font-semibold mb-2">Añadir nueva barbería</h3>
          <p class="text-sm text-gray-400 text-center mb-4">Configura un nuevo establecimiento para tu negocio</p>
          <button id="add-new-barbershop" class="bg-primary hover:bg-primary/90 text-white px-4 py-2 rounded-md">
            Crear barbería
          </button>
        </div>
      </div>

      <!-- Modal para Añadir/Editar Barbería (oculto por defecto) -->
      <div id="barbershop-modal" class="fixed inset-0 z-50 hidden">
        <div class="absolute inset-0 bg-black bg-opacity-50" id="modal-backdrop"></div>
        <div class="absolute inset-0 flex items-center justify-center p-4">
          <div class="bg-surface rounded-lg shadow-lg w-full max-w-3xl max-h-[90vh] overflow-y-auto">
            <div class="p-6">
              <div class="flex justify-between items-center mb-4">
                <h3 class="text-lg font-semibold" id="modal-title">Añadir Barbería</h3>
                <button id="close-modal" class="text-gray-400 hover:text-white">
                  <i class="fas fa-times"></i>
                </button>
              </div>

              <form id="barbershop-form">
                <div class="grid grid-cols-1 md:grid-cols-2 gap-6 mb-6">
                  <!-- Información Básica -->
                  <div class="md:col-span-2">
                    <h4 class="text-md font-medium mb-3 pb-2 border-b border-gray-700">Información Básica</h4>
                  </div>

                  <div>
                    <label for="barbershop-name" class="block text-sm text-gray-400 mb-1">Nombre de la Barbería *</label>
                    <input
                      type="text"
                      id="barbershop-name"
                      class="w-full bg-gray-700 border border-gray-600 rounded-md px-3 py-2 focus:outline-none focus:ring-2 focus:ring-primary"
                      placeholder="Ej: Barbería Moderna"
                      required
                    />
                  </div>

                  <div>
                    <label for="barbershop-phone" class="block text-sm text-gray-400 mb-1">Teléfono *</label>
                    <input
                      type="tel"
                      id="barbershop-phone"
                      class="w-full bg-gray-700 border border-gray-600 rounded-md px-3 py-2 focus:outline-none focus:ring-2 focus:ring-primary"
                      placeholder="Ej: +34 912 345 678"
                      required
                    />
                  </div>

                  <div>
                    <label for="barbershop-email" class="block text-sm text-gray-400 mb-1">Email</label>
                    <input
                      type="email"
                      id="barbershop-email"
                      class="w-full bg-gray-700 border border-gray-600 rounded-md px-3 py-2 focus:outline-none focus:ring-2 focus:ring-primary"
                      placeholder="Ej: info@barberiamoderna.com"
                    />
                  </div>

                  <div>
                    <label for="barbershop-website" class="block text-sm text-gray-400 mb-1">Sitio web</label>
                    <input
                      type="url"
                      id="barbershop-website"
                      class="w-full bg-gray-700 border border-gray-600 rounded-md px-3 py-2 focus:outline-none focus:ring-2 focus:ring-primary"
                      placeholder="Ej: https://www.barberiamoderna.com"
                    />
                  </div>

                  <!-- Dirección -->
                  <div class="md:col-span-2">
                    <h4 class="text-md font-medium mb-3 pb-2 border-b border-gray-700">Dirección</h4>
                  </div>

                  <div>
                    <label for="barbershop-address" class="block text-sm text-gray-400 mb-1">Dirección *</label>
                    <input
                      type="text"
                      id="barbershop-address"
                      class="w-full bg-gray-700 border border-gray-600 rounded-md px-3 py-2 focus:outline-none focus:ring-2 focus:ring-primary"
                      placeholder="Ej: Calle Gran Vía 28"
                      required
                    />
                  </div>

                  <div>
                    <label for="barbershop-city" class="block text-sm text-gray-400 mb-1">Ciudad *</label>
                    <input
                      type="text"
                      id="barbershop-city"
                      class="w-full bg-gray-700 border border-gray-600 rounded-md px-3 py-2 focus:outline-none focus:ring-2 focus:ring-primary"
                      placeholder="Ej: Madrid"
                      required
                    />
                  </div>

                  <div>
                    <label for="barbershop-postal" class="block text-sm text-gray-400 mb-1">Código Postal *</label>
                    <input
                      type="text"
                      id="barbershop-postal"
                      class="w-full bg-gray-700 border border-gray-600 rounded-md px-3 py-2 focus:outline-none focus:ring-2 focus:ring-primary"
                      placeholder="Ej: 28013"
                      required
                    />
                  </div>

                  <div>
                    <label for="barbershop-province" class="block text-sm text-gray-400 mb-1">Provincia *</label>
                    <input
                      type="text"
                      id="barbershop-province"
                      class="w-full bg-gray-700 border border-gray-600 rounded-md px-3 py-2 focus:outline-none focus:ring-2 focus:ring-primary"
                      placeholder="Ej: Madrid"
                      required
                    />
                  </div>

                  <!-- Horario -->
                  <div class="md:col-span-2">
                    <h4 class="text-md font-medium mb-3 pb-2 border-b border-gray-700">Horario</h4>
                  </div>

                  <div class="md:col-span-2 grid grid-cols-1 md:grid-cols-3 gap-4">
                    <div>
                      <label class="flex items-center mb-2">
                        <input type="checkbox" class="mr-2" checked />
                        <span>Lunes</span>
                      </label>
                      <div class="flex space-x-2">
                        <input
                          type="time"
                          class="flex-1 bg-gray-700 border border-gray-600 rounded-md px-2 py-1 text-sm focus:outline-none focus:ring-2 focus:ring-primary"
                          value="09:00"
                        />
                        <span class="text-gray-400">-</span>
                        <input
                          type="time"
                          class="flex-1 bg-gray-700 border border-gray-600 rounded-md px-2 py-1 text-sm focus:outline-none focus:ring-2 focus:ring-primary"
                          value="20:00"
                        />
                      </div>
                    </div>

                    <div>
                      <label class="flex items-center mb-2">
                        <input type="checkbox" class="mr-2" checked />
                        <span>Martes</span>
                      </label>
                      <div class="flex space-x-2">
                        <input
                          type="time"
                          class="flex-1 bg-gray-700 border border-gray-600 rounded-md px-2 py-1 text-sm focus:outline-none focus:ring-2 focus:ring-primary"
                          value="09:00"
                        />
                        <span class="text-gray-400">-</span>
                        <input
                          type="time"
                          class="flex-1 bg-gray-700 border border-gray-600 rounded-md px-2 py-1 text-sm focus:outline-none focus:ring-2 focus:ring-primary"
                          value="20:00"
                        />
                      </div>
                    </div>

                    <div>
                      <label class="flex items-center mb-2">
                        <input type="checkbox" class="mr-2" checked />
                        <span>Miércoles</span>
                      </label>
                      <div class="flex space-x-2">
                        <input
                          type="time"
                          class="flex-1 bg-gray-700 border border-gray-600 rounded-md px-2 py-1 text-sm focus:outline-none focus:ring-2 focus:ring-primary"
                          value="09:00"
                        />
                        <span class="text-gray-400">-</span>
                        <input
                          type="time"
                          class="flex-1 bg-gray-700 border border-gray-600 rounded-md px-2 py-1 text-sm focus:outline-none focus:ring-2 focus:ring-primary"
                          value="20:00"
                        />
                      </div>
                    </div>

                    <div>
                      <label class="flex items-center mb-2">
                        <input type="checkbox" class="mr-2" checked />
                        <span>Jueves</span>
                      </label>
                      <div class="flex space-x-2">
                        <input
                          type="time"
                          class="flex-1 bg-gray-700 border border-gray-600 rounded-md px-2 py-1 text-sm focus:outline-none focus:ring-2 focus:ring-primary"
                          value="09:00"
                        />
                        <span class="text-gray-400">-</span>
                        <input
                          type="time"
                          class="flex-1 bg-gray-700 border border-gray-600 rounded-md px-2 py-1 text-sm focus:outline-none focus:ring-2 focus:ring-primary"
                          value="20:00"
                        />
                      </div>
                    </div>

                    <div>
                      <label class="flex items-center mb-2">
                        <input type="checkbox" class="mr-2" checked />
                        <span>Viernes</span>
                      </label>
                      <div class="flex space-x-2">
                        <input
                          type="time"
                          class="flex-1 bg-gray-700 border border-gray-600 rounded-md px-2 py-1 text-sm focus:outline-none focus:ring-2 focus:ring-primary"
                          value="09:00"
                        />
                        <span class="text-gray-400">-</span>
                        <input
                          type="time"
                          class="flex-1 bg-gray-700 border border-gray-600 rounded-md px-2 py-1 text-sm focus:outline-none focus:ring-2 focus:ring-primary"
                          value="20:00"
                        />
                      </div>
                    </div>

                    <div>
                      <label class="flex items-center mb-2">
                        <input type="checkbox" class="mr-2" checked />
                        <span>Sábado</span>
                      </label>
                      <div class="flex space-x-2">
                        <input
                          type="time"
                          class="flex-1 bg-gray-700 border border-gray-600 rounded-md px-2 py-1 text-sm focus:outline-none focus:ring-2 focus:ring-primary"
                          value="10:00"
                        />
                        <span class="text-gray-400">-</span>
                        <input
                          type="time"
                          class="flex-1 bg-gray-700 border border-gray-600 rounded-md px-2 py-1 text-sm focus:outline-none focus:ring-2 focus:ring-primary"
                          value="14:00"
                        />
                      </div>
                    </div>
                  </div>

                  <!-- Imágenes -->
                  <div class="md:col-span-2">
                    <h4 class="text-md font-medium mb-3 pb-2 border-b border-gray-700">Imágenes</h4>
                  </div>

                  <div class="md:col-span-2">
                    <label class="block text-sm text-gray-400 mb-2">Imagen principal</label>
                    <div class="border-2 border-dashed border-gray-600 rounded-lg p-4 text-center">
                      <div class="flex flex-col items-center justify-center">
                        <i class="fas fa-cloud-upload-alt text-gray-400 text-3xl mb-2"></i>
                        <p class="text-sm text-gray-400 mb-2">Arrastra una imagen o haz clic para seleccionar</p>
                        <input type="file" class="hidden" id="main-image" accept="image/*" />
                        <button type="button" class="bg-gray-700 hover:bg-gray-600 text-sm px-4 py-2 rounded-md" onclick="document.getElementById('main-image').click()">
                          Seleccionar imagen
                        </button>
                      </div>
                    </div>
                  </div>

                  <!-- Configuración -->
                  <div class="md:col-span-2">
                    <h4 class="text-md font-medium mb-3 pb-2 border-b border-gray-700">Configuración</h4>
                  </div>

                  <div>
                    <label for="barbershop-status" class="block text-sm text-gray-400 mb-1">Estado</label>
                    <select id="barbershop-status" class="w-full bg-gray-700 border border-gray-600 rounded-md px-3 py-2 focus:outline-none focus:ring-2 focus:ring-primary">
                      <option value="active">Activa</option>
                      <option value="inactive">Inactiva</option>
                      <option value="pending">Pendiente</option>
                    </select>
                  </div>

                  <div>
                    <label for="barbershop-visibility" class="block text-sm text-gray-400 mb-1">Visibilidad</label>
                    <select id="barbershop-visibility" class="w-full bg-gray-700 border border-gray-600 rounded-md px-3 py-2 focus:outline-none focus:ring-2 focus:ring-primary">
                      <option value="public">Pública</option>
                      <option value="private">Privada</option>
                    </select>
                  </div>

                  <div class="md:col-span-2">
                    <label for="barbershop-description" class="block text-sm text-gray-400 mb-1">Descripción</label>
                    <textarea
                      id="barbershop-description"
                      rows="4"
                      class="w-full bg-gray-700 border border-gray-600 rounded-md px-3 py-2 focus:outline-none focus:ring-2 focus:ring-primary"
                      placeholder="Describe tu barbería..."
                    ></textarea>
                  </div>
                </div>

                <div class="flex justify-end space-x-3">
                  <button
                    type="button"
                    id="cancel-barbershop"
                    class="px-4 py-2 bg-gray-700 hover:bg-gray-600 rounded-md text-sm"
                  >
                    Cancelar
                  </button>
                  <button
                    type="submit"
                    class="px-4 py-2 bg-primary hover:bg-primary/90 text-white rounded-md text-sm"
                  >
                    Guardar
                  </button>
                </div>
              </form>
            </div>
          </div>
        </div>
      </div>
    </main>
  </div>
</div>

</body>

